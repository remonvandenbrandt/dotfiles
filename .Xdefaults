! URxvt*rxvt-unicode (urxvt) v9.20 - released: 2014-04-26

! URxvt.termName:    	rxvt
! URxvt.background:   #000000
! URxvt.foreground:   #cccccc
! URxvt.scrollBar: 	false
! URxvt.geometry: 	105x24
! 
! URxvt.urlLauncher: firefox

!-------------------------------------------------------------------------------
! Xft settings
!-------------------------------------------------------------------------------
 
Xft.dpi: 96
Xft.antialias: false
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight
 
URxvt.depth: 32
URxvt.geometry: 95x28
URxvt.transparent: false
URxvt.fading: 0
! URxvt.urgentOnBell: true
! URxvt.visualBell: true
URxvt.loginShell: true
URxvt.saveLines: 50
URxvt.internalBorder: 3
URxvt.lineSpace: 0
 
! Fonts
URxvt.allow_bold: false
/* URxvt.font: -*-terminus-medium-r-normal-*-12-120-72-72-c-60-iso8859-1 */
URxvt*font: xft:Monospace:pixelsize=13
URxvt*boldFont: xft:Monospace:pixelsize=13
 
! Fix font space
URxvt*letterSpace: -1
 
! Scrollbar
URxvt.scrollStyle: rxvt
URxvt.scrollBar: false
 
! Perl extensions
URxvt.perl-ext-common: default,matcher
URxvt.matcher.button: 1
URxvt.urlLauncher: firefox
 
! Cursor
URxvt.cursorBlink: true
URxvt.cursorColor: #657b83
URxvt.cursorUnderline: false
 
! Pointer
URxvt.pointerBlank: true

URxvt*foreground: #d1c3b8
URxvt*background: #2e2c28

